	  if test -f $(srcdir)/$$p; then \
	    echo " $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(gnome_terminal_helpdir)/$$p"; \
	    $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(gnome_terminal_helpdir)/$$p; \
	  else if test -f $$p; then \
	    echo " $(INSTALL_DATA) $$p $(DESTDIR)$(gnome_terminal_helpdir)/$$p"; \
	    $(INSTALL_DATA) $$p $(DESTDIR)$(gnome_terminal_helpdir)/$$p; \
	  fi; fi; \
	done

